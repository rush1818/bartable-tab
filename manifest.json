{
  "manifest_version": 2,

  "name": "Bartable-Tab",
  "description": "This extension allows you to view Bart map/schedules on the new tab page.",
  "version": "1.0",
  "permissions": ["tabs", "storage", "geolocation", "unlimitedStorage"
  ],
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  },
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" }
}
