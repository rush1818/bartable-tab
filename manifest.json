{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension allows to view Bart map/schedules on the new tab page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": ["tabs", "storage", "geolocation", "unlimitedStorage"
  ],
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  }
}
